    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/web-worker": "^1.6.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
