{
  "name": "stan-playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "experimentalTernaries": false,
    "bracketSameLine": false,
    "arrowParens": "always"
  },
  "scripts": {
