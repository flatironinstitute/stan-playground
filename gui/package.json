{
  "name": "stan-playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "experimentalTernaries": false,
    "bracketSameLine": false,
    "arrowParens": "always"
  },
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "test-watch": "vitest"
